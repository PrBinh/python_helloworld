print("Kiem tra so nguyen to")
a = int(input("Nhap vao 1 so: "))
if a==0 or a==1:
    print("%s khong la so nguyen to" %(a))
else:
    if a==2:
        print("2 la so nguyen to")
    else:
        b=2
        while(a>b):
            if a%b==0 & a!=b:
                 print("%s Khong la so nguyen to" %(a))
                 break
            else:
                b=b+1
                if b==a:
                    print("%s la so nguyen to" %(a))

